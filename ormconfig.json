{
   "name": "default",
   "type": "sqlite",
   "synchronize": false,
   "logging": true,
   "database": "./database/database.sqlite",
   "logger": "simple-console",
   "entities": [
      "./electron/database/models/*.{ts,js}"
   ],
   "migrations": [
      "./database/migration/*.{ts,js}"
   ],
   "cli": {
      "entitiesDir": "./src/electron/database/models",
      "migrationsDir": "./database/migration"
   }
}
